@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery antiforgery
@{
    var tokens = antiforgery.GetAndStoreTokens(Context); // stores cookie + gives request token
}
@if (!User.Identity.IsAuthenticated)
{
    <script>
        window.location.href = '@Url.Action("Login", "Account", new { area = "", returnUrl = Context.Request.Path })';
    </script>
}

@{
    var userId = User.FindFirstValue(ClaimTypes.NameIdentifier);
    var email = User.FindFirstValue(ClaimTypes.Email);
    var userName = User.FindFirstValue(ClaimTypes.Name);
    var fullName = HttpContextAccessor.HttpContext.Session.GetString("name") ?? "";
    // var lastName = HttpContextAccessor.HttpContext.Session.GetString("lastName");

}

