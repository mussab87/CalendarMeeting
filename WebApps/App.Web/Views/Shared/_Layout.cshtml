<head>
    <script>
        // make token and header name globally available
        window.AntiForgeryToken = '@tokens.RequestToken';
        window.AntiForgeryHeaderName = 'X-CSRF-TOKEN'; // must match options.HeaderName
    </script>

    <partial name="_HeadPartial" />

    <script>
        // optional: globally attach header to jQuery AJAX requests
        if (window.jQuery && window.AntiForgeryToken) {
            $.ajaxSetup({
                headers: {
                    [window.AntiForgeryHeaderName]: window.AntiForgeryToken
                }
            });
            console.log("AjaxSetup added header:", window.AntiForgeryHeaderName);
        }
    </script>
</head>

<!--end::Head-->

